{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {"browsers": ["last 2 versions", "ie >= 9"]}
      }
    ],
    "react"
  ],
  "plugins": ["react-hot-loader/babel", "lodash", "transform-object-rest-spread"],
  "env": {
    "production": {
      "presets": ["react-optimize"]
    }
  }
}
