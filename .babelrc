{
  "presets": [
    ["env", { "targets": { "browsers": ["last 2 versions", "ie >= 9"] } }],
    "react"
  ],
  "plugins": [
    "lodash",
    "transform-object-rest-spread",
    "react-hot-loader/babel"
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ]
    }
  }
}
